{
  "expo": {
    "name": "Silagora",
    "slug": "Silagora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "lesouffle",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDummy_Key_For_Development_Only"
      },
      "bundleIdentifier": "com.anonymous.Silagora"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDummy_Key_For_Development_Only"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.anonymous.Silagora"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-location"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
